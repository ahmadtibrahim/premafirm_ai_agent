<odoo>
    <record id="view_fleet_vehicle_form_premafirm" model="ir.ui.view">
        <field name="name">fleet.vehicle.form.premafirm</field>
        <field name="model">fleet.vehicle</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="inside">
                <group string="Dispatch Defaults">
                    <group>
                        <field name="x_studio_gvore"/>
                        <field name="x_studio_height_ft"/>
                        <field name="x_studio_current_load_lbs"/>
                        <field name="x_studio_x_is_busy"/>
                    </group>
                    <group>
                        <field name="x_studio_location"/>
                        <field name="x_studio_service_type"/>
                        <field name="x_studio_load_type"/>
                    </group>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
