<odoo>

    <record id="view_premafirm_load_stop_tree" model="ir.ui.view">
        <field name="name">premafirm.load.stop.tree</field>
        <field name="model">premafirm.load.stop</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="sequence"/>
                <field name="stop_type"/>
                <field name="address"/>
                <field name="pallets"/>
                <field name="weight_lbs"/>
                <field name="x_studio_pickup_dt"/>
                <field name="x_studio_delivery_dt"/>
            </tree>
        </field>
    </record>

</odoo>
